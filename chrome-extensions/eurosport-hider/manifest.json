{
    "manifest_version": 2,
    "name": "name",
    "version": "1.0",
    "description": "desc",
    "browser_action": { "default_icon": "icon.png" },
    "permissions": [ "activeTab", "tabs", "http://*/*" ],
    "background": {
      "scripts": ["content.js"],
      "persistent": false
    },
    "content_scripts": [ {
        "all_frames": true,
        "js": [ "content.js" ],
        "matches": [ "http://*/*", "https://*/*" ] 
    } ]
}
